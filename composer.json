{
    "name": "archielite/laravel-heroicons",
    "description": "The Heroicons Blade Component",
    "keywords": [
        "archielite",
        "laravel-heroicons"
    ],
    "homepage": "https://github.com/archielite/laravel-heroicons",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Archi Elite",
            "email": "contact@archielite.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0|^8.1",
        "illuminate/support": "^8.0|^9.0",
        "illuminate/view": "^8.0|^9.0"
    },
    "require-dev": {
        "ext-zip": "*",
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0",
        "illuminate/filesystem": "^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "ArchiElite\\LaravelHeroicons\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ArchiElite\\LaravelHeroicons\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "ArchiElite\\LaravelHeroicons\\LaravelHeroiconsServiceProvider"
            ]
        }
    }
}
